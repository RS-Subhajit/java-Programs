package abc;

public class abc
{
int x,y,sum;
 void get(int m,int n)
 {
   x=m;
   y=n;
 }
   void put()
   {
       sum=x+y;
       System.out.println("x is:"+x);
       System.out.println("y is:"+y); 
        System.out.println("sum is:"+sum);
   }
 
}
  class xyz
    {
    public static void main(String[] args)
        {
        abc obj;
        obj=new abc();
                obj.get(6,7);
                obj.put();
        }
    }

run:
x is:6
y is:7
